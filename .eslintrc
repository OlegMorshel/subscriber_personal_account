{
  "extends": ["plugin:prettier/recommended"],
  "rules": {
    "max-len": [
      "error",
      {
        "code": 140
      }
    ],
    "no-duplicate-imports": 2,
    "multiline-ternary": "error",
    "no-label-var": "warn",
    "prettier/prettier": "error"
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "plugins": ["prettier"],
  "env": {
    "es6": true
  }
}
